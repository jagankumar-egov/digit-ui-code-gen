    "url": "https://github.com/egovernments/digit-module-generator"
  },
  "bugs": {
    "url": "https://github.com/egovernments/digit-module-generator/issues"
  },
  "homepage": "https://github.com/egovernments/digit-module-generator#readme",
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.6.0",
    "boxen": "^5.1.2",
    "case": "^1.6.3",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
